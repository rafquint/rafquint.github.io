                        "Inciteful": "https://placehold.co/150x150/e83e8c/ffffff?text=INC",
                        "Paperpal": "https://placehold.co/150x150/007bff/ffffff?text=PPAL",
                        "Swarm": "https://upload.wikimedia.org/wikipedia/commons/4/4d/OpenAI_Logo.svg"
                    };
                    return logos[name] || 'https://placehold.co/150x150/6c757d/ffffff?text=IA';
                };
                
                const getLogoWithFallback = (name) => {
                    return getLogoPath(name) || getLogoPath2(name);
                };
                
                const erroredLogos = ref({});

                const onLogoError = (event, name) => {
                    if (!erroredLogos.value[name]) {
                        erroredLogos.value[name] = true;
                        event.target.src = getLogoPath2(name);
                    } else {
                        event.target.src = 'https://placehold.co/150x150/6c757d/ffffff?text=IA';
                    }
                };
                
                // Computed properties
                const filteredAIs = computed(() => {
                    let result = ais.value;
                    
                    // Apply type filter
                    if (activeTypeFilter.value !== 'all') {
                        result = result.filter(ai => ai.type === activeTypeFilter.value);
                    }
                    
                    // Apply difficulty filter
                    if (activeDifficultFilter.value !== 'all') {
                        result = result.filter(ai => ai.difficulty === activeDifficultFilter.value);
                    }
                    
                    // Apply favorites filter
                    if (showOnlyFavorites.value) {
                        result = result.filter(ai => favorites.value.includes(ai.id));
                    }
                    
                    // Apply academic filter
                    if (academicOnly.value) {
                        result = result.filter(ai => ai.academic === true);
                    }
                    
                    // Apply search filter
                    if (searchQuery.value) {
                        const query = normalizeString(searchQuery.value.trim());
                        result = result.filter(ai => 
                            normalizeString(ai.name).includes(query) || 
                            normalizeString(ai.description).includes(query) ||
                            normalizeString(ai.developer).includes(query) ||
                            (ai.features && ai.features.some(feature => feature.toLowerCase().includes(query)))
                        );
                    }
                    
                    // Sort alphabetically
                    return result.sort((a, b) => a.name.localeCompare(b.name));
                });
                
                const normalizeString = (str) => {
                    return str
                        .toLowerCase()
                        .normalize('NFD')
                        .replace(/[\u0300-\u036f]/g, '');
                };
                
                // Favorite methods
                const toggleFavorite = (aiId) => {
                    const index = favorites.value.indexOf(aiId);
                    
                    if (index === -1) {
                        favorites.value.push(aiId);
                    } else {
                        favorites.value.splice(index, 1);
                    }
                    
                    localStorage.setItem('aiFavorites', JSON.stringify(favorites.value));
                };
                
                const isFavorite = (aiId) => {
                    return favorites.value.includes(aiId);
                };
                
                const toggleAcademicFilter = () => {
                    academicOnly.value = !academicOnly.value;
                    // For√ßar reatividade
                    filterAIs();
                };

                const toggleFavoritesFilter = () => {
                    showOnlyFavorites.value = !showOnlyFavorites.value;
                    // For√ßar reatividade
                    filterAIs();
                };
                
                // Filter methods
                const setTypeFilter = (filter) => {
                    activeTypeFilter.value = filter;
                };
                
                const setDifficultFilter = (filter) => {
                    activeDifficultFilter.value = filter;
                };
                
                const resetFilter = () => {
                    searchQuery.value = '';
                    activeTypeFilter.value = 'all';
                    activeDifficultFilter.value = 'all';
                    academicOnly.value = false;
                    showOnlyFavorites.value = false;
                };
                
                const filterAIs = () => {
                    isLoading.value = true;
                    // Simulate loading delay for better UX
                    setTimeout(() => {
                        isLoading.value = false;
                    }, 300);
                };
				
                /*const handleQuestion = async () => {
                    const question = userQuestion.value.trim().toLowerCase();
                
                    // Busca por palavras-chave simples nos nomes, descri√ß√µes e features
                    const results = ais.value.filter(ai =>
                        ai.name.toLowerCase().includes(question) ||
                        ai.description.toLowerCase().includes(question) ||
                        ai.features.some(f => f.toLowerCase().includes(question))
                    );
                
                    if (results.length > 0) {
                        answer.value = `Encontrei ${results.length} IA(s) relacionadas:\n` + results.map(ai => `- ${ai.name}: ${ai.description}`).join('\n');
                    } else {
                        answer.value = "Desculpe, n√£o encontrei nenhuma IA relevante para essa pergunta.";
                    }
                };*/
                const handleQuestion = async () => {
                    answer.value = "Processando...";
                    try {
                        const output = await pipeline(userQuestion.value);
                        answer.value = output[0].generated_text;
                    } catch (e) {
                        answer.value = "Erro ao processar sua pergunta.";
                        console.error(e);
                    }
                };

                
                return {
                    ais,
                    searchQuery,
                    activeTypeFilter,
                    activeDifficultFilter,
                    currentDate,
                    filteredAIs,
                    isLoading,
                    setTypeFilter,
                    setDifficultFilter,
                    resetFilter,
                    filterAIs,
                    toggleFavorite,
                    isFavorite,
                    toggleAcademicFilter,
                    toggleFavoritesFilter,
                    getLogoPath,
                    getLogoPath2,
                    getLogoWithFallback,
                    onLogoError,
                    showOnlyFavorites,
                    academicOnly,
					userQuestion,
					answer,
					handleQuestion,
					modelLoadError,
                };
            }
        }).mount('#app');
    </script>
