                    //pipeline = await window.transformers.pipeline('text2text-generation', 'https://github.com/rafquint/rafquint.github.io/upload/main/models/t5-small');
					try {
						pipeline = await window.transformers.pipeline('text2text-generation', './models/t5-small');
					} catch (e) {
						console.error('Erro ao carregar o modelo:', e);
